<div class="container">
      <h1><%= form_title %>:</h1>
      <% if @patient.errors.any? %>
        <div class="alert alert-danger">
          <ul>
            <% @patient.errors.full_messages.each do |msg| %>
              <li><%= msg %></li>
            <% end %>
          </ul>
        </div>
      <% end %>

      <%= form_for @patient do |f| %>
        <div class="row">
          <div class="col-md-3">
            <div class="form-group">
              <%= f.label :consult_date, "Fecha de la consulta:" %>
              <%= f.date_field :consult_date, class: 'form-control' %>
            </div>
          </div>
          <div class="col-md-3">
            <div class="form-group">
              <%= f.label :city, "Ciudad de la consulta:" %>
              <%= f.text_field :city, class: "form-control input-lg", placeholder: "Ingresa la ciudad", :autocomplete => :off %>
            </div>
          </div>
          <div class="col-md-3">
            <div class="form-group">
              <%= f.label :patient_name, "Nombre completo del paciente:" %>
              <%= f.text_field :patient_name, class: "form-control input-lg", placeholder: "Ingresa nombre paciente", :autocomplete => :off %>
            </div>
          </div>
          <div class="col-md-3">
            <div class="form-group">
              <%= f.label :number_document, "Número de documento del paciente:" %>
              <%= f.text_field :number_document, class: "form-control input-lg", placeholder: "Ingresa cedula", :autocomplete => :off %>
            </div>
          </div>
          <div class="col-md-3">
            <div class="form-group">
              <%= f.label :age, "Edad del paciente:" %>
              <%= f.text_field :age, class: "form-control input-lg", placeholder: "Ingresa edad", :autocomplete => :off %>
            </div>
          </div>
          <div class="col-md-3">
            <div class="form-group">
              <%= f.label :scholarship, "Escolaridad:" %>
              <%= f.select :scholarship, [["Escolaridad"], ["Sin Estudios"], ["Bachillerato"], ["Técnico"], ["Profesional"], ["Profesional Especializado"]], class: "form-control input-lg" %>
              <%= f.label :civil_status, "Estado Civil:" %>
              <%= f.select :civil_status, [["Estado Civil"], ["Soltero/a"], ["Casado/a"], ["Separado/a"], ["Viudao/a"]], class: "form-control input-lg" %>
            </div>
          </div>
          <div class="col-md-3">
            <div class="form-group">
              <%= f.label :children, "Número de hijos:" %>
              <%= f.text_field :children, class: "form-control input-lg", placeholder: "Numero de Hijos", :autocomplete => :off %>
            </div>
          </div>
          <div class="col-md-3">
            <div class="form-group">
              <%= f.label :time_work, "Tiempo total de trabajo en dias:" %>
              <%= f.text_field :time_work, class: "form-control input-lg", placeholder: "Tiempo total de trabajo", :autocomplete => :off %>
            </div>
          </div>
          <div class="col-md-3">
            <div class="form-group">
