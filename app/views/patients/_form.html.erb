              <%= f.text_field :current_work, class: "form-control input-lg", placeholder: "Puesto de trabajo Actual", :autocomplete => :off %>
            </div>
          </div>
          <div class="col-md-3">
            <div class="form-group">
              <%= f.label :years_at_work, "Años en el puesto de trabajo:" %>
              <%= f.text_field :years_at_work, class: "form-control input-lg", placeholder: "Años en el puesto de trabajo", :autocomplete => :off %>
            </div>
          </div>
          <div class="col-md-3">
            <div class="form-group">
              <%= f.label :area_belongs, "Area a la que pertenece" %>
              <%= f.text_field :area_belongs, class: "form-control input-lg", placeholder: "Area  la que pertenece", :autocomplete => :off %>
            </div>
          </div>
          <div class="col-md-3">
            <div class="form-group">
              <%= f.label :hours_work, "Horas diarias de trabajo" %>
              <%= f.text_field :hours_work, class: "form-control input-lg", placeholder: "Horas diarias de trabajo", :autocomplete => :off %>
            </div>
          </div>
          <div class="col-md-4">
            <div class="form-group">
              <%= f.label :vinculation, "Tipo de vinculacion laboral:" %>
              <%= f.select :vinculation, [["vinculacion laboral"], ["Fijo"], ["Contratado"]], class: "btn btn-default" %>
            </div>
            <div class="form-group">
              <%= f.label :schedule, "Horario:" %>
              <%= f.select :schedule, [["Horario"], ["Fijo"], ["Rotativo"], ["Descansa fines de semana"]], class: "btn btn-default" %>
            </div>
          </div>
          <div class="col-md-4">
            <div class="form-group">
              <%= f.label :description_work, "Descripcion del cargo desempeñado:" %>
              <%= f.text_area :description_work, class: "form-control input-lg", placeholder: "Describa el cargo que desempeña", :autocomplete => :off %>
            </div>
          </div>
          <div class="col-md-4">
            <div class="form-group">
              <%= f.label :make_your_work, "Realiza su trabajo generalmente:" %>
              <%= f.select :make_your_work, [["Realiza su trabajo"], ["De pie"], ["Sentado"], ["De pie con esfuerzo fisico"], ["De pie caminando"], ["Utilizando herramientas manuales"], ["Usando maquinas herramientas"]], class: "btn btn-default" %>
