            <div class="form-group">
              <%= f.label :job_requires, "Su trabajo le exige:" %>
              <%= f.select :job_requires, [["Su trabajo le exige"], ["Esfuerzo fisico"], ["Repetitividad"], ["Posturas forzadas a nivel de columna"], ["Moviemientos repetitivos"], ["Actividades de vibracion corporal en el trabajo"]], class: "btn btn-default" %>
            </div>
          </div>
          <div class="col-md-4">
            <div class="form-group">
              <%= f.label :previous_jobs, "Trabajos desarrollados con anterioridad:" %>
              <%= f.text_area :previous_jobs, class: "form-control input-lg", placeholder: "Trabajos desarrollados con anteriordad", :autocomplete => :off %>
            </div>
          </div>
          <div class="col-md-4">
            <div class="form-group">
              <%= f.label :extra_work, "Tiempo extra laboral:" %>
              <%= f.select :extra_work, [["Dedica su tiempo a"], ["Actividades domesticas"], ["Hobby"], ["Recreacion"], ["Practica de deporte o ejercicios fisicos"], ["Actividades laborales, remuneracion extra"], ["Descanso en la vivienda"]], class: "btn btn-default" %>
            </div>
          </div>
          <div class="col-md-4">
            <div class="form-group">
              <%= f.label :healt_data, "Datos de salud:" %>
              <%= f.text_area :healt_data, class: "form-control input-lg", placeholder: "Datos de Salud", :autocomplete => :off %>
            </div>
          </div>
          <div class="col-md-12">
            <div class="form-group">
              <%= f.label :current_symptoms, "Sintomas actuales:" %>
              <%= f.text_area :current_symptoms, class: "form-control input-lg", placeholder: "Sintomas Actuales", :autocomplete => :off %>
            </div>
          </div>
        </div>
